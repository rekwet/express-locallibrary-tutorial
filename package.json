{
  "name": "my-local-library",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": "^8.11.1"
  },
  "scripts": {
    "start": "node $npm_package_config_entrypoint",
    "debug": "node --inspect=0.0.0.0:9229 $npm_package_config_entrypoint",
    "debug:brk": "node --inspect-brk=0.0.0.0:9229 $npm_package_config_entrypoint",
    "debug:legacy": "node --debug=0.0.0.0:5858 $npm_package_config_entrypoint",
    "test": "nyc mocha --exit",
    "devstart": "nodemon $npm_package_config_entrypoint"
  },
  "nodemonConfig": {
    "env": {
      "NODE_HEAPDUMP_OPTIONS": "nosignal",
      "NODE_ENV": "production"
    }
  },
  "config": {
    "entrypoint": "./bin/www"
  },
  "dependencies": {
    "appmetrics-dash": "^4.1.0",
    "appmetrics-prometheus": "^2.0.0",
    "appmetrics-zipkin": "^1.1.1",
    "async": "^2.6.2",
    "body-parser": "^1.18.3",
    "cookie-parser": "^1.4.4",
    "debug": "^2.6.9",
    "express": "~4.16.0",
    "express-validator": "^5.3.1",
    "helmet": "^3.16.0",
    "http-errors": "~1.6.2",
    "log4js": "^4.0.2",
    "moment": "^2.24.0",
    "mongoose": "^5.4.19",
    "morgan": "~1.9.0",
    "pug": "^2.0.3",
    "yaml-update": "^0.1.2"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "eslint": "^5.15.3",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "mocha": "^6.0.2",
    "nodemon": "^1.18.10",
    "nyc": "^13.3.0"
  }
}
